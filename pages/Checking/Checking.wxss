page {
  background: #f19dad;
}

.container {
  padding: 0px;
}

.logo {
  width: 165rpx;
  height: 45rpx;
  margin: 50rpx auto 40rpx;
  display: block;
}

.conn {
  width: 100%;
  margin: 300rpx auto 60rpx;
}

.title {
  color: #fff;
  font-size: 48rpx;
  text-align: center;
  font-weight: bold;
  display: block;
  margin-bottom: 140rpx;
}

.section {
  width: 80%;
  height: 100rpx;
  background: #fff;
  border-radius: 50rpx;
  display: flex;
  align-items: center;
  justify-content: flex-start;
  margin: 40rpx auto;
  color: #aeaeae;
}

.section picker {
  width: 100%;
  height: 100%;
}

.picker {
  width: 90%;
  height: 100rpx;
  display: flex;
  align-items: center;
  padding-left: 60rpx;
  box-sizing: border-box;
}

.picker input {
  width: 100%;
  height: 100%;
}

.enquiry {
  width: 80% !important;
  height: 100rpx;
  line-height: 100rpx;
  background: none;
  border: 1rpx solid #fff;
  border-radius: 50rpx;
  color: #fff;
  padding: 0;
  margin: 0;
}
